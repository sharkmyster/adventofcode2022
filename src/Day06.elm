module Day06 exposing (..)

import Set

markerSize : Int
markerSize =
    14


allDifferent : List Char -> Bool
allDifferent list =
    let
        set =
            Set.fromList list
    in
    Set.size set == markerSize


parseInput : Int -> List Char -> Int
parseInput count input =
    let
        sublist =
            List.take markerSize input
    in
    if allDifferent sublist then
        count + markerSize

    else if input == [] then
        0

    else
        parseInput (count + 1) (List.drop 1 input)


part1 : Int
part1 =
    rawData
        |> String.toList
        |> parseInput 0


rawData : String
rawData =
    ""
